    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "flow_relu3_2"
  type: "ReLU"
  bottom: "flow_conv3_2"
  top: "flow_conv3_2"
}
layer {
  name: "flow_conv3_3"
  type: "Convolution"
  bottom: "flow_conv3_2"
  top: "flow_conv3_3"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 3
  }
