  type: "ReLU"
  bottom: "flow_conv5_1"
  top: "flow_conv5_1"
}
layer {
  name: "flow_conv5_2"
  type: "Convolution"
  bottom: "flow_conv5_1"
  top: "flow_conv5_2"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "flow_relu5_2"
  type: "ReLU"
  bottom: "flow_conv5_2"
  top: "flow_conv5_2"
}
layer {
  name: "flow_conv5_3"
  type: "Convolution"
  bottom: "flow_conv5_2"
  top: "flow_conv5_3"
  param {
    lr_mult: 1
