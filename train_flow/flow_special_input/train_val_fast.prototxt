  bottom: "flow_pool3"
  top: "flow_conv4_1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
  #propagate_down: false
}
layer {
  name: "flow_relu4_1"
  type: "ReLU"
  bottom: "flow_conv4_1"
  top: "flow_conv4_1"
}
layer {
  name: "flow_conv4_2"
  type: "Convolution"
  bottom: "flow_conv4_1"
  top: "flow_conv4_2"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "flow_relu4_2"
  type: "ReLU"
  bottom: "flow_conv4_2"
  top: "flow_conv4_2"
}
layer {
  name: "flow_conv4_3"
  type: "Convolution"
  bottom: "flow_conv4_2"
  top: "flow_conv4_3"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "flow_relu4_3"
  type: "ReLU"
  bottom: "flow_conv4_3"
  top: "flow_conv4_3"
}
layer {
  name: "flow_pool4"
