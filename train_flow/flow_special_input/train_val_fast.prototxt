  }
}
layer {
  name: "flow_relu5_3"
  type: "ReLU"
  bottom: "flow_conv5_3"
  top: "flow_conv5_3"
}
layer {
  name: "flow_pool5"
  type: "Pooling"
  bottom: "flow_conv5_3"
  top: "flow_pool5"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}

layer {
  name: "flow_fc6"
  type: "InnerProduct"
  bottom: "flow_pool5"
  top: "flow_fc6"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "flow_relu6"
  type: "ReLU"
  bottom: "flow_fc6"
  top: "flow_fc6"
}
layer {
  name: "flow_drop6"
  type: "Dropout"
  bottom: "flow_fc6"
  top: "flow_fc6"
  dropout_param {
    dropout_ratio: 0.9
  }
}
layer {
  name: "flow_fc7"
  type: "InnerProduct"
  bottom: "flow_fc6"
  top: "flow_fc7"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "flow_relu7"
  type: "ReLU"
  bottom: "flow_fc7"
  top: "flow_fc7"
}
layer {
  name: "flow_drop7"
  type: "Dropout"
  bottom: "flow_fc7"
  top: "flow_fc7"
  dropout_param {
    dropout_ratio: 0.8
  }
}
layer {
  name: "flow_fc8"
  type: "InnerProduct"
  bottom: "flow_fc7"
  top: "flow_fc8"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 101
    weight_filler {
      type: "gaussian"
	  std: 0.001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "flow_loss"
  type: "SoftmaxWithLoss"
  bottom: "flow_fc8"
  bottom: "label"
  include {
    phase: TRAIN
  }
}
layer {
  name: "flow_loss"
  type: "SoftmaxWithLoss"
  bottom: "flow_fc8"
  bottom: "label"
  top: "flow_loss"
  include {
    phase: TEST
  }
}
layer {
  name: "flow_accuracy"
  type: "Accuracy"
  bottom: "flow_fc8"
  bottom: "label"
  top: "flow_accuracy"
  include {
    phase: TEST
  }
}
