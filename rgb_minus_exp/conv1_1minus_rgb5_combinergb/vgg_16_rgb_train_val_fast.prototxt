  name: "rgb_relu7"
  type: "ReLU"
  bottom: "rgb_fc7"
  top: "rgb_fc7"
}
layer {
  name: "rgb_drop7"
  type: "Dropout"
  bottom: "rgb_fc7"
  top: "rgb_fc7"
  dropout_param {
    dropout_ratio: 0.9
  }
}
layer {
  name: "rgb_fc8_rgb"
  type: "InnerProduct"
  bottom: "rgb_fc7"
  top: "rgb_fc8"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 101
    weight_filler {
      type: "gaussian"
      std: 0.001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "rgb_loss"
  type: "SoftmaxWithLoss"
  bottom: "rgb_fc8"
  bottom: "rgb_label"
  include {
    phase: TRAIN
  }
}
layer {
  name: "rgb_loss"
  type: "SoftmaxWithLoss"
  bottom: "rgb_fc8"
  bottom: "rgb_label"
  top: "rgb_loss"
  include {
    phase: TEST
  }
}
layer {
  name: "rgb_accuracy"
  type: "Accuracy"
  bottom: "rgb_fc8"
  bottom: "rgb_label"
  top: "rgb_accuracy"
  include {
    phase: TEST
  }
}
