name: "rgb_CUHK_Action_TemporalNet"
layer {
  name: "rgb_data"
  type: "VideoData"
  top: "rgb_data"
  top: "rgb_label"
  video_data_param {
    source: "dataset_file_examples/train_rgb_split1.txt"
    batch_size: 50
    new_width: 340
    new_height: 256
    new_length: 1
    num_segments: 1
    modality: RGB
  }
  transform_param{
    crop_size: 224
    mirror: true
    fix_crop: true
    multi_scale: true
    max_distort: 1
    scale_ratios: [1,.875,.75,.66]
    mean_value: [104, 117, 123]
  }
  include: { phase: TRAIN }
}
layer {
  name: "rgb_data"
  type: "VideoData"
  top: "rgb_data"
  top: "rgb_label"
  video_data_param {
    source: "dataset_file_examples/val_rgb_split1.txt"
    batch_size: 5
    new_length: 1
    new_height: 256
    new_width: 340
    num_segments: 1
    modality: RGB
    
    full_test: false
    full_test_param {
      is_mirror: false
      is_crop: false
      full_test_num_segments: 25
      # in full test mode, you must set test_batch_size and batch_size is invalid
      test_batch_size: 5
    }
  }
  transform_param{
    crop_size: 224
    mirror: false
    mean_value: [104, 117, 123]
  }
  include: { phase: TEST }
}
layer {
  name: "rgb_conv1_1"
  type: "Convolution"
  bottom: "data_p2"
  top: "rgb_conv1_1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "rgb_relu1_1"
  type: "ReLU"
  bottom: "rgb_conv1_1"
  top: "rgb_conv1_1"
}
layer {
  name: "rgb_conv1_2"
  type: "Convolution"
  bottom: "rgb_conv1_1"
  top: "rgb_conv1_2"
